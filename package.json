{
  "name": "histograph-import",
  "version": "0.1.0",
  "description": "Imports data into Histograph",
  "main": "index.js",
  "scripts": {
    "test": "jshint index.js && jscs index.js"
  },
  "bin": {
    "histograph-import": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/histograph/import.git"
  },
  "author": "Bert Spaan",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/histograph/import/issues"
  },
  "homepage": "https://github.com/histograph/import",
  "dependencies": {
    "async": "^0.9.0",
    "colors": "^1.0.3",
    "histograph-config": "histograph/config",
    "minimist": "^1.1.2",
    "request": "^2.53.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "jscs": "^1.10.0",
    "jshint": "^2.6.0"
  },
  "jshintConfig": {
    "node": true,
    "globalstrict": false,
    "undef": true,
    "unused": true,
    "noarg": true
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requireCamelCaseOrUpperCaseIdentifiers": null,
    "disallowMultipleVarDecl": true,
    "requireMultipleVarDecl": null
  }
}
